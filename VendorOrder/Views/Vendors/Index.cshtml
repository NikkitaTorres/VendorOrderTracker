@model List<VendorOrder.Models.Vendor>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


@{
    ViewData["Title"] = "Vendors";
}

<h2>Vendors</h2>

<ul>
    @foreach (var vendor in Model)
    {
        <li>
            <a asp-controller="Vendors" asp-action="Details" asp-route-id="@vendor.VendorId">@vendor.Name</a>
        </li>
    }
</ul>

<a asp-controller="Vendors" asp-action="Create">Create New Vendor</a>
